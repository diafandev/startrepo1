{
  "name": "moovin-challenge-node",
  "version": "0.0.0",
  "description": "Moovin Challenge Node.js",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts",
    "commit": "git add .; git-cz",
    "husky:prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "Moovin",
    "Node.js"
  ],
  "author": "Leonardo Campello",
  "mainScs":"branch",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faunadb": "^4.4.1",
    "uuidv4": "^6.2.12"
  }
}
